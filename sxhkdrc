    alacritty -e /bin/bash --init-file $RICE_DIR/bashrc

super + space
    rofi -config $RICE_DIR/nord.rasi -show drun

super + q
    bspc node -c

XF86AudioMute
    amixer set Master toggle

super + Tab
    bspc desktop -f last

